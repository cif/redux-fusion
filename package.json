{
  "name": "redux-fusion",
  "version": "0.1.7",
  "description": "RxJS Redux Bindings",
  "main": "fusion.js",
  "scripts": {
    "build": "npm run peers && babel ./src -d ./ --ignore **/*.test.js",
    "mocha": "mocha ./src/*.test.js --compilers js:babel-core/register --reporter dot",
    "lint": "node_modules/eslint/bin/eslint.js src",
    "peers": "node node_modules/npm-install-peers/dist/index.js",
    "prepublish": "npm build",
    "test": "npm run mocha && npm run lint"
  },
  "author": "email@benipsen.com",
  "repository": "https://github.com/cif/redux-fusion.git",
  "license": "ISC",
  "dependencies": {
    "react": "^15.4.2",
    "recompose": ">= 0.22.0",
    "rxjs": "^5.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "redux": "^3.6.0",
    "sinon": "^2.1.0"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "recompose": ">= 0.22.0",
    "rxjs": "^5.2.0"
